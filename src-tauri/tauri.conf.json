{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "ProManage",
  "version": "0.1.0",
  "identifier": "com.promanage.app",
  "build": {
    "beforeDevCommand": "npm --prefix ./ProManageFR run dev:both",
    "beforeBuildCommand": "npm --prefix ./ProManageFR run build:both",
    "devUrl": "http://localhost:19006",
    "frontendDist": "../ProManageFR/dist"
  },
  "app": {
    "windows": [
      {
        "title": "ProManage",
        "width": 1280,
        "height": 800,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all"
  }
}
